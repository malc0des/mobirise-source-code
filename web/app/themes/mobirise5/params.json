{
    "title": "Mobirise 5",
    "description": "Description",
    "name": "mobirise5",
    "largeCover": "cover.jpg",
    "version": 1,
    "minAppVersion": "5.0.0",
    "includes": [
        "core-summernote-dropdown-menu-bs-5",
        "arrow-down",
        "core-accordion-bootstrap",
        "core-toggle-bootstrap",
        "dropdown",
        "gallery-video",
        "scroll-to-top",
        "scroll-animation",
        "core-after-navbar",
        "core-auto-paddings",
        "menu-styles",
        "core-menu-styles",
        "gallery",
        "core-gallery",
        "testimonials",
        "core-testimonials",
        "slider-mobirise",
        "core-slider-mobirise",
        "core-dropdown",
        "item-wizard"
    ],
    "theme": {
        "type": "secondary",
        "base": {
            "name": "bootstrap",
            "version": 5
        },
        "settings": [{
                "type": "title",
                "title": "Site Styles"
            },
            {
                "type": "subtitle",
                "title": "Colors"
            },
            {
                "var": "primaryColor",
                "type": "color",
                "title": "Primary",
                "groupName": "colors",
                "default": "#6592e6"
            },
            {
                "type": "colors",
                "title": "Buttons and Links",
                "groupName": "colors",
                "set": [{
                        "var": "secondaryColor",
                        "default": "#ff6666"
                    },
                    {
                        "var": "successColor",
                        "default": "#40b0bf"
                    },
                    {
                        "var": "infoColor",
                        "default": "#47b5ed"
                    },
                    {
                        "var": "warningColor",
                        "default": "#FFE161"
                    },
                    {
                        "var": "dangerColor",
                        "default": "#ff9966"
                    }
                ]
            },
            {
                "type": "subtitle",
                "title": "Fonts"
            },
            {
                "var": "mainFont",
                "default": "Jost"
            },
            {
                "type": "group",
                "rule": ".mbr-fonts-style, [mbr-buttons], [mbr-menu]",
                "title": "Title 1",
                "name": "display-1",
                "set": [{
                        "type": "font",
                        "var": "display1Font",
                        "default": "Jost"
                    },
                    {
                        "type": "range",
                        "min": 0.4,
                        "max": 12,
                        "step": 0.2,
                        "var": "display1Size",
                        "default": 4.6
                    }
                ]
            },
            {
                "type": "group",
                "rule": ".mbr-fonts-style, [mbr-buttons], [mbr-menu]",
                "title": "Title 2",
                "name": "display-2",
                "set": [{
                        "type": "font",
                        "var": "display2Font",
                        "default": "Jost"
                    },
                    {
                        "type": "range",
                        "min": 0.4,
                        "max": 6,
                        "step": 0.2,
                        "var": "display2Size",
                        "default": 3
                    }
                ]
            },
            {
                "type": "group",
                "rule": ".mbr-fonts-style, [mbr-buttons], [mbr-menu]",
                "title": "Title 3",
                "name": "display-5",
                "set": [{
                        "type": "font",
                        "var": "display5Font",
                        "default": "Jost"
                    },
                    {
                        "type": "range",
                        "min": 0.4,
                        "max": 2.5,
                        "step": 0.2,
                        "var": "display5Size",
                        "default": 2
                    }
                ]
            },
            {
                "type": "group",
                "rule": ".mbr-fonts-style, [mbr-buttons], [mbr-menu]",
                "title": "Text",
                "name": "display-7",
                "set": [{
                        "type": "font",
                        "var": "display7Font",
                        "default": "Jost"
                    },
                    {
                        "type": "range",
                        "min": 0.4,
                        "max": 2,
                        "step": 0.2,
                        "var": "display7Size",
                        "default": 1.2
                    }
                ]
            },
            {
                "type": "group",
                "rule": ".mbr-fonts-style, [mbr-buttons], [mbr-menu]",
                "title": "Menu",
                "name": "display-4",
                "set": [{
                        "type": "font",
                        "var": "display4Font",
                        "default": "Jost"
                    },
                    {
                        "type": "range",
                        "min": 0.4,
                        "max": 2,
                        "step": 0.2,
                        "var": "display4Size",
                        "default": 1.1
                    }
                ]
            },
            {
                "type": "subtitle",
                "title": "Options"
            },
            {
                "var": "isRoundedButtons",
                "type": "switch",
                "title": "Rounded Buttons",
                "default": false
            },
            {
                "var": "isGhostButtonBorder",
                "type": "switch",
                "title": "Ghost Button Border",
                "default": false
            },
            {
                "var": "underlinedLinks",
                "type": "switch",
                "title": "Underlined Links",
                "default": false
            },
            {
                "var": "isAnimatedOnScroll",
                "type": "switch",
                "title": "Animation on Scroll",
                "default": false
            },
            {
                "var": "isScrollToTopButton",
                "type": "switch",
                "title": "Scroll to Top Button",
                "default": false
            }
        ],
        "fonts": [{
            "url": "https://fonts.googleapis.com/css2?family=Jost:ital,wght@0,400;0,700;1,400;1,700&display=swap",
            "name": "Jost",
            "css": "'Jost', sans-serif"
        }],
        "buttonColors": {
            "btn-primary": "@primaryColor",
            "btn-secondary": "@secondaryColor",
            "btn-success": "@successColor",
            "btn-info": "@infoColor",
            "btn-warning": "@warningColor",
            "btn-danger": "@dangerColor",
            "btn-white": "#fff",
            "btn-black": "#000"
        },
        "hasButtonOutlineColors": true,
        "linkColors": {
            "text-primary": "@primaryColor",
            "text-secondary": "@secondaryColor",
            "text-success": "@successColor",
            "text-info": "@infoColor",
            "text-warning": "@warningColor",
            "text-danger": "@dangerColor",
            "text-white": "#fff",
            "text-black": "#000"
        },
        "colorsSet": [
            "#4479D9", "#E43F3F", "#2299AA", "#22A5E5", "#E6C63B", "#FFB18A", "#ffffff",
            "#6592e6", "#ff6666", "#40b0bf", "#47b5ed", "#FFE161", "#ff9966", "#fafafa",
            "#8CAFF0", "#FF8A8A", "#65C6D4", "#6EC7F2", "#FFE885", "#232323", "#bbbbbb",
            "#BED3F9", "#FFB5B5", "#98DEE8", "#9FDBF8", "#FFF0B0", "#000000", "#353535"
        ],
        "customLESS": "plugins/theme/css/custom.less"
    },
