            "#D70081","#FFD7EF","#320707","#9C98CF","#393193","#A6A99C","#F7F7F7",
            "#1F6463","#A0E1E1","#260A30","#FFA0A0","#F92C50","#757B62","#232323",
            "#645600","#FFEB69","#3A341C","#FFC091","#FFA600","#BCEF9B","#000000"
        ],
